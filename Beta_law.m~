
% Beta law

x=-2:0.1:2;
y=(1+x).^2;
plot(x,y)
grid on; hold on
% xlim([1 N*2+2])
ay = gca;
ay.YTick = [-2 -1 0 1 2];
ay.YTickLabel = {'','14','','N','','14','0'};
set(gca, 'fontsize', 14, 'linewidth', 2)
xlabel('p^{trans}','fontsize',14)
ylabel('A/A0','fontsize',14)
% legend('Arteries','Capillaries', 'Veins')